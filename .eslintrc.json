{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "comma-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "no-use-before-define": 0,
        "prefer-const": 0,
        "strict": 0,
        "no-underscore-dangle": 0
    }
}